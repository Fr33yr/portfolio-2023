{
  "extends": ["next/core-web-vitals", "airbnb", "plugin:prettier/recommended"],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": "lastest",
    "sourceType": "module"
  },
  "rules": {
    "max-len": ["error", 140],
    "quotes": [2, "single", { "avoidEscape": true }],
    "no-console": "warn"
  }
}
